    "df_mexico = pd.read_csv(io.StringIO(response.decode('utf-8')), skiprows=4, skipfooter=7, engine='python') # con skiprows salteamos las primeras y últimas filas que generan errores\n",
    "\n",
    "# Observamos\n",
    "df_mexico"
   ]

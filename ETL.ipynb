   "metadata": {},
   "outputs": [],
   "source": [
    "# Se crean las variables que contienen url de la pagina a raspar, como a su vez realizar la peticion a la pagina\n",
    "website = 'https://www.data.jma.go.jp/multi/quake/index.html?lang=es'\n",
    "path = 'driver\\chromedriver.exe'\n",
    "driver = webdriver.Chrome(path)\n",
    "driver.get(website)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Utilizamos la libreria time para darle tiempo al driver de extraer la informacion de la pagina, colocando una espera implicita de 5 segundos\n",
    "time.sleep(5)\n",
    "\n",
    "# Extraemos la informacion de la pagina\n",
    "tabla = driver.find_element_by_xpath(\"//table[@id='quakeindex_table']\")\n",
    "filas = tabla.find_elements_by_xpath(\".//tr\")\n",
    "\n",
    "# Se crean las variables donde se alamacenaran los datos de cada columna\n",
    "fecha = []\n",
    "lugar = []\n",
    "magnitud = []\n",
    "intensidad = []\n",
    "fecha2 = []\n",
    "# Iterar sobre las filas de la tabla y obtener los datos de las celdas\n",
    "for fila in filas:\n",
    "    celdas = fila.find_elements_by_xpath(\".//td\")\n",
    "    # Ignorar la primera fila que solo contiene los nombres de las columnas\n",
    "    if celdas:\n",
    "        datos_fila = []\n",
    "        for celda in celdas:\n",
    "            datos_fila.append(celda.text)\n",
    "        # Se agregan los datos en las variables creadas\n",
    "        fecha.append(datos_fila[0])\n",
    "        lugar.append(datos_fila[1])\n",
    "        magnitud.append(datos_fila[2])\n",
    "        intensidad.append(datos_fila[3])\n",
    "        fecha2.append(datos_fila[4])\n",
    "        \n",
    "# Cerramos la pagina\n",
    "driver.quit()"
   ]
  },

   "source": [
    "# Creamos una función que nos ayude a crear tablas dimensionales\n",
    "def crear_tabla_dimensional(tabla, columna):\n",
    "    # Extraemos la columna y generar un nuevo DataFrame\n",
    "    columna_extraida = tabla[columna].unique()\n",
    "    tabla_dimensional = pd.DataFrame({columna: columna_extraida})\n",
    "    \n",
    "    # Agregamos un ID incremental\n",
    "    tabla_dimensional[f'id_{columna}'] = range(1, len(tabla_dimensional) + 1)\n",
    "\n",
    "    # Reemplazamos la columna en la tabla original con el ID correspondiente\n",
    "    tabla[columna] = tabla[columna].map(tabla_dimensional.set_index(columna)[f'id_{columna}'])\n",
    "\n",
    "    # Reemplazamos el nombre de la columna por el nombre del ID\n",
    "    tabla.rename(columns={ columna : f'id_{columna}' },inplace=True)\n",
    "    \n",
    "    return tabla_dimensional"
   ]
